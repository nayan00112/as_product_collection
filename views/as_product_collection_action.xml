<?xml version="1.0" encoding="utf-8"?>
<odoo>

    
    <record id="as_product_collection_action" model="ir.actions.act_window">
        <field name="name">Product Collection 1</field>
        <field name="res_model">as.product.collection</field>
        <field name="view_mode">list,form</field>
    </record>

    <record id="as_product_collection_view_list" model="ir.ui.view">
        <field name="name">as.product.collection.view.list</field>
        <field name="model">as.product.collection</field>
        <field name="arch" type="xml">
            <list string="Product Collection">
                <field name="name" />
                <field name = "product_collection_ids" />
            </list>
        </field>
    </record>

</odoo>
